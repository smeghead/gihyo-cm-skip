{
  "name": "Giyo.jp CM skip",
  "version": "1.0.1",
  "description": "gihyo.jp のエントリを閲覧する前に表示される広告をスキップします。",
  "icons": { "48": "icon.png" },
  "content_scripts": [
    {
      "matches": [
        "http://gihyo.jp/*"
      ],
      "js": ["gihyo.js"],
      "run_at": "document_end"
    }
  ]
}
