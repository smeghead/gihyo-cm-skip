{
  "name": "CM skip",
  "version": "1.2.0",
  "description": "gihyo.jp とマイナビニュースのエントリを閲覧する前に表示される広告をスキップします。",
  "icons": { "48": "icon.png" },
  "manifest_version": 2,
  "content_scripts": [
    {
      "matches": [
        "http://gihyo.jp/*",
        "http://journal.mycom.co.jp/*",
        "http://news.mynavi.jp/*"
      ],
      "js": ["gihyo.js"],
      "run_at": "document_start"
    }
  ]
}
